<div
  data-hook="playable-time-control"
  class="${props.styles.timeWrapper} ${props.themeStyles.timeText}">
  <span
    data-hook="playable-current-time-indicator"
    class="${props.styles.time}">
  </span>
  <span
    data-hook="playable-duration-time-indicator"
    class="${props.styles.time} ${props.styles.duration}">
  </span>
</div>
