{
  "name": "playable",
  "version": "1.2.1",
  "author": {
    "name": "Oleksii Makodzeba",
    "email": "oleksiim@wix.com"
  },
  "main": "dist/statics/playable.bundle.js",
  "module": "dist/src/index.js",
  "jsnext:main": "dist/src/index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "start": "PORT=5000 haste start --entry-point=./dev-env/start-dev-env.js",
    "lint": "npm run tslint:all",
    "build": "haste build",
    "build:production": "NODE_ENV=production haste build",
    "test": "npm run test:unit",
    "documentation": "okidoc ./docs/docs.yml ./docs",
    "documentation:gitadd": "npm run documentation && git add ./docs",
    "documentation:site": "okidoc-site develop ./docs/site.yml",
    "documentation:site:build": "npm run documentation && okidoc-site build ./docs/site.yml",
    "documentation:site:deploy": "npm run documentation:site:build && gh-pages -d sitedist",
    "version": "node scripts/npm-version.js",
    "postversion": "git push && git push --tags",
    "release": "node scripts/npm-release.js",
    "tslint": "ts-node node_modules/.bin/tslint -p . -c tslint.json",
    "tslint:all": "npm run tslint -- '{src,scripts}/**/*.ts'",
    "prettier": "prettier --config .prettierrc --write",
    "prettier:all": "npm run prettier -- '{src,scripts,tslint-rules}/**/*.{js,ts}'",
    "csscomb:all": "csscomb src/.",
    "test:playback": "node ./dev-env/boot-devserver-and-test.js",
    "test:unit": "haste test --mocha",
    "precommit": "lint-staged && npm run build && npm run test:unit && npm run test:playback && npm run documentation:gitadd",
    "rollup": "node rollup/build.js",
    "rollup:production": "NODE_ENV=production node rollup/build.js"
  },
  "lint-staged": {
    "*.ts": [
      "prettier",
      "tslint",
      "git add"
    ],
    "*.scss": [
      "csscomb -tty-mode --",
      "git add"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "css-element-queries": "^0.4.0",
    "dashjs": "latest",
    "eventemitter3": "^2.0.3",
    "hls.js": "latest",
    "tslib": "^1.9.0"
  },
  "devDependencies": {
    "@babel/types": "7.0.0-beta.38",
    "@types/chai": "^4.0.8",
    "@types/mocha": "^2.2.44",
    "autoprefixer": "^7.2.5",
    "babylon": "7.0.0-beta.34",
    "chai": "~3.5.0",
    "css-modules-loader-core": "^1.1.0",
    "csscomb": "^4.2.0",
    "documentation": "^5.3.3",
    "ejs": "^2.5.5",
    "express": "^4.14.0",
    "gh-pages": "^1.1.0",
    "glob": "^7.1.2",
    "haste-preset-yoshi": "git+ssh://git@github.com/bodia-uz/wix-haste-preset-yoshi-video-player.git#215166c",
    "husky": "^0.13.1",
    "jsdom-global": "^2.1.1",
    "lint-staged": "^5.0.0",
    "modular-css-rollup": "^7.0.0",
    "node-sass": "^4.7.2",
    "okidoc": "^1.1.0",
    "postcss-clean": "^1.1.0",
    "postcss-modules": "^1.1.0",
    "prettier": "^1.9.0",
    "rollup": "^0.55.4",
    "rollup-plugin-collect-sass": "^1.0.9",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-dot": "^1.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "^0.5.5",
    "rollup-plugin-scss": "^0.3.0",
    "rollup-plugin-typescript2": "^0.8.1",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-plugin-url": "^1.3.0",
    "sinon": "^2.3.8",
    "tslint": "^5.8.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^2.7.0"
  },
  "haste": {
    "preset": "yoshi"
  },
  "yoshi": {
    "separateCss": false,
    "optimizeMoment": true,
    "exports": "Playable",
    "entry": {
      "playable": "./index.ts",
      "playable-test": "./e2e/playback-test.ts",
      "playable-develop": "./develop.ts",
      "playable-dash": "./with-dash.ts",
      "playable-hls": "./with-hls.ts"
    }
  },
  "keywords": [
    "player",
    "video",
    "media",
    "html5",
    "mse",
    "adaptive",
    "hls",
    "dash",
    "mpeg"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:wix/playable.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "logLevel": "verbose"
  }
}
